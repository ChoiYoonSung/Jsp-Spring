<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
			PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
			"http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="member">
	
	<select id="loginCheckId" parameterClass="String" resultClass="int">
		SELECT 	count(*) cnt
		FROM MEMBER_TEST
		WHERE MEM_ID = #memId#
	</select>
	
	<select id="login" parameterClass="com.servlet.vo.MemberVO" resultClass="com.servlet.vo.MemberVO">
		select
			  MEM_ID	memId
			, MEM_PW	memPw
		from MEMBER_TEST
		where MEM_ID = #memId#
		and MEM_PW = #memPw#
	</select>
	
</sqlMap>